<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="VO.foodcategoryVO" table="foodcategory">
   <id name="foodcatid" type="long" column="ID" >
   <generator class="increment"/>
  </id>

  <property name="foodcatName">
   <column name="foodcategoryname" />
  </property>
  
  <property name="foodcatDes">
  <column name="foodcategorydescription"/>
  </property>
 </class>
</hibernate-mapping>
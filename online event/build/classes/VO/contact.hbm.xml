<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="VO.contactVO" table="contact">
   <id name="contactid" type="long" column="ID" >
   <generator class="increment"/>
  </id>
 <property name="contactname">
   <column name="name" />
  </property>
  <property name="contactmessage">
  <column name="message"/>
  </property>
  
  <property name="contactemail">
  <column name="email"/>
  </property>
 </class>
</hibernate-mapping>